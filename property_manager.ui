/* 左侧工具栏 */
QWidget#tool {
    background-color: #4080FF; 
    border-radius: 10px 0 0 10px;
    min-width: 225px; 
    max-width: 225px;
    padding: 15px 0;
    box-shadow: 2px 0 10px rgba(64, 128, 255, 0.2);
}

/* 工具按钮 */
QWidget#tool QToolButton {
    border: none;
    background-color: transparent;
    color: white;
    font-family: &quot;微软雅黑&quot;;
    font-size: 24pt;
    min-height: 80px;
    width: 215px;
    text-align: left;
    padding-left: 35px;
    icon-size: 28px;
    icon-text-spacing: 15px;
    border-radius: 6px;
    transition: all 0.2s;
}

QWidget#tool QToolButton:hover:!checked {
    background-color: rgba(255, 255, 255, 0.15);
    padding-left: 38px;
}

QWidget#tool QToolButton:checked {
    background-color: white;
    color: #4080FF;
    font-weight: bold;
    padding-left: 32px;
}

/* 标题标签 */
QLabel[name=&quot;ti&quot;] {
    color: #165DFF;
    font-family: &quot;微软雅黑&quot;;
    font-size: 18pt;
    font-weight: bold;
    padding: 15px 0 20px 10px;
    border-bottom: 1px solid #C9E0FF;
}

QLabel[name=&quot;lb&quot;] {
    color: #333;
    font-family: &quot;微软雅黑&quot;;
    font-size: 13pt;
    padding: 8px 0;
}

/* 按钮 */
QPushButton[name=&quot;pu&quot;] {
    background-color: #4080FF;
    color: white;
    border: none;
    border-radius: 8px;
    padding: 12px 20px;
    font-family: &quot;微软雅黑&quot;;
    font-size: 13pt;
    min-height: 0;
    min-width: 0;
    margin: 0 5px;
    transition: all 0.2s;
}

QPushButton[name=&quot;pu&quot;]:hover {
    background-color: #165DFF;
    box-shadow: 0 3px 8px rgba(64, 128, 255, 0.3);
}

QPushButton[name=&quot;pu&quot;]:pressed {
    background-color: #0E42D2;
    transform: translateY(1px);
}

/* 次要按钮 */
QPushButton[name=&quot;pu&quot;][text=&quot;查询&quot;],
QPushButton[name=&quot;pu&quot;][text=&quot;刷新&quot;],
QPushButton[name=&quot;pu&quot;][text=&quot;搜索&quot;] {
    background-color: white;
    color: #4080FF;
    border: 1px solid #C9E0FF;
}

QPushButton[name=&quot;pu&quot;][text=&quot;查询&quot;]:hover,
QPushButton[name=&quot;pu&quot;][text=&quot;刷新&quot;]:hover,
QPushButton[name=&quot;pu&quot;][text=&quot;搜索&quot;]:hover {
    background-color: #F0F7FF;
    border-color: #A8D0FF;
}

/* 输入框 */
QLineEdit[name=&quot;le&quot;] {
    border: 1px solid #B9D4FF;
    border-radius: 8px;
    padding: 12px 15px;
    font-family: &quot;微软雅黑&quot;;
    font-size: 13pt;
    min-height: 0;
    min-width: 0;
    background-color: white;
}

QLineEdit[name=&quot;le&quot;]:focus {
    border-color: #4080FF;
    box-shadow: 0 0 0 3px rgba(64, 128, 255, 0.2);
}

/* 下拉框 */
QComboBox {
    border: 1px solid #B9D4FF;
    border-radius: 8px;
    padding: 12px 15px;
    font-family: &quot;微软雅黑&quot;;
    font-size: 13pt;
    min-height: 0;
    background-color: white;
}

/* 表格 */
QTableView {
    border: 1px solid #C9E0FF;
    border-radius: 8px;
    background-color: white;
    font-family: &quot;微软雅黑&quot;;
    font-size: 12pt;
    min-width: 0;
    min-height: 0;
    gridline-color: #F0F7FF;
}

QTableView::item {
    padding: 15px 12px;
    min-height: 0;
    border-bottom: 1px solid #F0F7FF;
}

QTableView::item:selected {
    background-color: #E8F0FE;
    color: #4080FF;
}

QHeaderView::section {
    background-color: #F0F7FF;
    color: #4080FF;
    font-weight: bold;
    font-size: 13pt;
    padding: 15px 12px;
    border-bottom: 2px solid #C9E0FF;
    min-height: 0;
}

/* 内容区容器 */
QStackedWidget {
    background-color: white;
    border-radius: 0 10px 10px 0;
    padding: 20px;
    margin: 5px 5px 5px 0;
    width: 100%;
    height: 100%;
}

/* 功能区容器 */
QWidget[name=&quot;bg&quot;] {
    background-color: white;
    border-radius: 8px;
    padding: 15px;
    width: 100%;
    height: 100%;
}


    

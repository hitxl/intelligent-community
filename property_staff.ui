  <property name="styleSheet">
   <string notr="true">/* 基础样式设置 - 大幅缩小非侧边栏元素 */
QWidget {
    font-family: &quot;微软雅黑&quot;, &quot;Microsoft YaHei UI&quot;;
    font-size: 18px; /* 全局字体进一步缩小 */
    color: #23395B;
    border-radius: 6px; /* 进一步缩小圆角 */
}

/* 主窗口背景 */
QWidget#centralwidget {
    background-color: #DCEEFF;
    padding: 10px; /* 大幅缩小内边距 */
}

/* 左侧侧边栏 - 固定宽度，内部按钮样式不变 */
QWidget#tool {
    background-color: #2A85FF;
    border-radius: 12px 0 0 12px;
    padding: 30px 0;
    min-width: 220px; 
    max-width: 220px;
}

/* 侧边栏工具按钮 - 保持原有尺寸，取消字体加粗 */
QWidget#tool QToolButton {
    border: none;
    background-color: transparent;
    color: rgba(255, 255, 255, 0.95);
    min-height: 80px; /* 原高度不变 */
    width: 220px;     /* 原宽度不变 */
    font-size: 24px;  /* 原字体不变 */
    font-weight: normal; /* 显式取消加粗 */
    text-align: left;
    padding-left: 20px;
    margin: 12px 10px;
    border-radius: 12px;
    transition: all 0.3s ease;
}

/* 侧边栏按钮交互效果（保持不变） */
QWidget#tool QToolButton:hover {
    background-color: rgba(255, 255, 255, 0.2);
}
QWidget#tool QToolButton:pressed,
QWidget#tool QToolButton:checked {
    background-color: rgba(255, 255, 255, 0.3);
}

/* 主按钮样式 - 大幅缩小尺寸 */
QPushButton {
    background-color: #337AB7;
    color: white;
    border: none;
    border-radius: 6px;
    padding: 10px 15px; /* 大幅缩小内边距 */
    font-size: 16px;    /* 大幅缩小字体 */
    min-height: 40px;   /* 大幅缩小按钮高度 */
    min-width: 100px;   /* 大幅缩小按钮宽度 */
    transition: all 0.3s ease;
}
QPushButton:hover {
    background-color: #286090;
    box-shadow: 0 3px 10px rgba(40, 96, 144, 0.3);
}
QPushButton:pressed {
    background-color: #1F4E79;
    transform: translateY(2px);
}

/* 主内容区容器 */
QStackedWidget {
    border: 1px solid #B9D4FF;
    border-radius: 6px;
    margin: 15px; /* 大幅缩小外边距 */
    padding: 20px; /* 大幅缩小内边距 */
    background-color: white;
    box-shadow: 0 3px 12px rgba(0, 0, 0, 0.08);
}

/* 输入控件 - 大幅缩小尺寸 */
QLineEdit, QComboBox, QTextEdit {
    border: 1px solid #99C5FF;
    border-radius: 6px;
    padding: 10px 15px; /* 大幅缩小内边距 */
    background-color: white;
    font-size: 16px;    /* 大幅缩小字体 */
    min-height: 40px;   /* 大幅缩小高度 */
    transition: border 0.3s ease, box-shadow 0.3s ease;
}
QLineEdit:focus, QComboBox:focus, QTextEdit:focus {
    border-color: #337AB7;
    box-shadow: 0 0 0 4px rgba(51, 122, 183, 0.2);
}

/* 表格控件 - 大幅缩小尺寸 */
QTableView {
    background-color: white;
    alternate-background-color: #E6F2FF;
    border: 1px solid #B9D4FF;
    border-radius: 6px;
    padding: 10px; /* 大幅缩小内边距 */
    gridline-color: #D0E8FF;
    font-size: 14px; /* 大幅缩小字体 */
}
QHeaderView::section {
    background-color: #E6F2FF;
    color: #23395B;
    padding: 10px; /* 大幅缩小内边距 */
    border: 1px solid #D0E8FF;
    font-weight: bold; /* 表格头部保持加粗（如需取消可改 normal） */
    font-size: 14px; /* 大幅缩小字体 */
}
QTableView::row {
    height: 30px; /* 大幅缩小行高 */
}

/* 标签页控件 - 大幅缩小尺寸 */
QTabBar::tab {
    background-color: #E6F2FF;
    color: #4A6FA5;
    padding: 10px 20px; /* 大幅缩小内边距 */
    border-radius: 6px 6px 0 0;
    margin-right: 6px;
    font-size: 14px; /* 大幅缩小字体 */
    min-width: 100px; /* 大幅缩小标签宽度 */
}
QTabBar::tab:selected {
    background-color: white;
    color: #2A85FF;
    font-weight: bold;
}
QTabWidget::pane {
    border: 1px solid #B9D4FF;
    border-radius: 0 6px 6px 6px;
    padding: 15px; /* 大幅缩小内边距 */
    background-color: white;
}

/* 分组框 - 大幅缩小尺寸 */
QGroupBox {
    border: 1px solid #B9D4FF;
    border-radius: 6px;
    margin-top: 15px; /* 大幅缩小上边距 */
    padding: 15px; /* 大幅缩小内边距 */
    font-weight: bold;
    color: #2A85FF;
    font-size: 16px; /* 大幅缩小字体 */
}
QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    padding: 0 10px;
    top: -12px;
    left: 25px;
    background-color: white;
}

/* 标签样式 - 大幅缩小字体 */
QLabel {
    font-size: 16px; /* 大幅缩小标签字体 */
    color: #23395B;
    padding: 5px 0; /* 大幅缩小上下间距 */
}
QLabel#title {
    font-size: 24px; /* 大幅缩小标题字体 */
    font-weight: bold;
    color: #2A85FF;
    margin-bottom: 15px;
}

/* 进度条样式 - 大幅缩小尺寸 */
QProgressBar {
    border: 1px solid #B9D4FF;
    border-radius: 6px;
    text-align: center;
    height: 25px; /* 大幅缩小高度 */
    font-size: 14px; /* 大幅缩小字体 */
}
QProgressBar::chunk {
    background-color: #2A85FF;
    border-radius: 5px;
}</string>

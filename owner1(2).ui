   <string notr="true">/* 主窗口样式 - 统一浅蓝色背景 */
QWidget#Owner {
    background-color: #f0f7ff;
    padding: 15px;
}

/* 左侧侧边栏容器 - 降低饱和度的蓝色 */
QWidget#widget {
    background-color: #4080ff; /* 降低饱和度的蓝色 */
    border-radius: 8px 0 0 8px;
    padding: 20px 0;
    box-shadow: 2px 0 8px rgba(64, 128, 255, 0.15); /* 调整阴影颜色与侧边栏匹配 */
}
QWidget#widget {
    background-color: #2A85FF;
    border-radius: 12px 0 0 12px;
    padding: 30px 0;
    min-width: 220px; 
    max-width: 220px;
}

/* 侧边栏工具按钮 - 与侧边栏同宽且保持圆角 */
QWidget#widget QToolButton {
    border: none;
    background-color: transparent;
    color: rgba(255, 255, 255, 0.9);
    min-height: 80px;
    width: 150px;
    font-family: &quot;微软雅黑&quot;, &quot;Microsoft YaHei UI&quot;;
    font-size: 24px;
    text-align: left;
    padding-left: 25px;
    margin: 3px 5px;
    border-radius: 6px;
    transition: all 0.2s;
min-width: 180px; 
    max-width: 180px;
}

QWidget#widget QToolButton::icon {
    width: 24px;
    height: 24px;
    margin-right: 10px;
    filter: brightness(100);
}

QWidget#widget QToolButton:hover {
    background-color: rgba(255, 255, 255, 0.15);
    padding-left: 27px;
}

QWidget#widget QToolButton:checked {
    background-color: white;
    color: #4080ff; /* 选中状态文字颜色与侧边栏匹配 */
    font-weight: 500;
    padding-left: 23px;
}

/* 按钮样式 - 统一蓝色主题 */
QPushButton {
    background-color: #165dff; /* 保持原主按钮颜色不变，突出重点操作 */
    color: white;
    border: none;
    border-radius: 6px;
    padding: 12px 22px;
    font-family: &quot;微软雅黑&quot;, &quot;Microsoft YaHei UI&quot;;
    font-size: 17px;
    min-height: 50px;
    min-width: 110px;
    transition: all 0.2s;
}

QPushButton:hover {
    background-color: #4080ff; /* 悬停时与侧边栏颜色一致 */
    box-shadow: 0 3px 8px rgba(22, 93, 255, 0.25);
}

QPushButton:pressed {
    background-color: #0e42d2;
    transform: translateY(1px);
}

/* 次要按钮 - 浅蓝色背景 */
QPushButton#pushButton_2, QPushButton#pushButton_4, QPushButton#pushButton_6 {
    background-color: #e8f3ff;
    color: #4080ff; /* 次要按钮文字颜色与侧边栏匹配 */
    border: 1px solid #c9e0ff;
}

QPushButton#pushButton_2:hover, QPushButton#pushButton_4:hover, QPushButton#pushButton_6:hover {
    background-color: #d6eaff;
    box-shadow: 0 3px 8px rgba(64, 128, 255, 0.15);
}

/* 标签样式 - 层次分明 */
QLabel {
    font-family: &quot;微软雅黑&quot;, &quot;Microsoft YaHei UI&quot;;
    font-size: 17px;
    color: #333;
    padding: 8px 0;
}

QLabel[text=&quot;故障报修&quot;], QLabel[text=&quot;维修进度查询&quot;], QLabel[text=&quot;维修记录与评价&quot;],
QLabel[text=&quot;我的车位&quot;], QLabel[text=&quot;申请车位&quot;], QLabel[text=&quot;空闲车位查询&quot;],
QLabel[text=&quot;车辆识别&quot;], QLabel[text=&quot;业主id&quot;], QLabel[text=&quot;故障反馈&quot;],
QLabel[text=&quot;维修记录&quot;], QLabel[text=&quot;业主姓名&quot;], QLabel[text=&quot;车位申请记录&quot;] {
    font-weight: bold;
    color: #4080ff; /* 标题文字颜色与侧边栏匹配 */
    font-size: 19px;
    margin: 10px 0;
}

/* 输入框样式 - 蓝色边框与交互 */
QLineEdit {
    font-family: &quot;微软雅黑&quot;, &quot;Microsoft YaHei UI&quot;;
    font-size: 17px;
    border: 1px solid #b9d4ff;
    border-radius: 6px;
    padding: 12px 15px;
    background-color: white;
    min-height: 50px;
    min-width: 200px;
}

QLineEdit:focus {
    border-color: #69b1ff;
    outline: none;
    box-shadow: 0 0 0 3px rgba(105, 177, 255, 0.2);
}

/* 文本编辑框 - 与输入框统一风格 */
QTextEdit {
    font-family: &quot;微软雅黑&quot;, &quot;Microsoft YaHei UI&quot;;
    font-size: 17px;
    border: 1px solid #b9d4ff;
    border-radius: 6px;
    padding: 12px;
    background-color: white;
    min-height: 150px;
}

QTextEdit:focus {
    border-color: #69b1ff;
    box-shadow: 0 0 0 3px rgba(105, 177, 255, 0.2);
}

/* 表格视图 - 浅蓝色背景与蓝色边框 */
QTableView {
    gridline-color: #e0e0e0;
    border: 1px solid #b9d4ff;
    border-radius: 6px;
    font-family: &quot;微软雅黑&quot;, &quot;Microsoft YaHei UI&quot;;
    font-size: 17px;
    background-color: white;
    min-height: 280px;
    min-width: 550px;
    padding: 6px;
}

QTableView::item {
    padding: 14px 16px;
    border-bottom: 1px solid #f0f0f0;
    min-height: 50px;
}

QTableView::item:selected {
    background-color: #e8f0fe;
    color: #4080ff; /* 表格选中状态文字颜色与侧边栏匹配 */
    font-weight: 500;
}

QHeaderView::section {
    background-color: #f0f7ff;
    border-bottom: 1px solid #b9d4ff;
    padding: 15px 16px;
    font-weight: bold;
    color: #4080ff; /* 表头文字颜色与侧边栏匹配 */
    font-size: 18px;
    min-height: 55px;
    text-align: left;
}

/* 标签页样式 - 与整体背景协调 */
QTabWidget::pane {
    border: 1px solid #b9d4ff;
    border-radius: 6px;
    background-color: white;
    margin-top: 5px;
    padding: 18px;
}

QTabBar::tab {
    background-color: #f0f7ff;
    border: none;
    border-radius: 6px 6px 0 0;
    padding: 14px 22px;
    font-family: &quot;微软雅黑&quot;, &quot;Microsoft YaHei UI&quot;;
    font-size: 17px;
    margin-right: 5px;
    min-width: 130px;
    min-height: 45px;
}

QTabBar::tab:selected {
    background-color: white;
    border-top: 3px solid #4080ff; /* 选中标签页边框颜色与侧边栏匹配 */
}

QTabBar::tab:hover:!selected {
    background-color: #e6f2ff;
}

/* 滚动条样式 - 蓝色主题 */
QScrollBar:vertical {
    width: 14px;
    margin: 6px 0;
}

QScrollBar:horizontal {
    height: 14px;
    margin: 0 6px;
}

QScrollBar::handle {
    background-color: #b9d4ff;
    border-radius: 7px;
    min-height: 35px;
    min-width: 35px;
}

QScrollBar::handle:hover {
    background-color: #69b1ff;
}

/* 布局间距 - 优化空白区域 */
QGridLayout, QHBoxLayout, QVBoxLayout {
    spacing: 15px;
    margin: 12px;
}

/* 进度指示器图标 - 增大尺寸 */
QLabel[styleSheet*=&quot;border-image: url(:/qt6/diamond2.png)&quot;] {
    width: 28px;
    height: 28px;
}

QLabel[styleSheet*=&quot;border-image: url(:/qt6/arrow.png)&quot;],
QLabel[styleSheet*=&quot;border-image: url(:/qt6/rightarrow.png)&quot;] {
    font-size: 20px;
}</string>
